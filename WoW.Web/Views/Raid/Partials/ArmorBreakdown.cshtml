@model WoW.Models.Raid.RaidCompModel

<div class="panel">
    <div class="row">
        <h3>Armor Types</h3>
        <div class="large-12 medium-12 columns">
            <div class="large-12 medium-12 columns">
                <div class="large-3 medium-3 columns"><p>Cloth: @Model.ClothWearers</p></div>
                
                <div class="large-3 medium-3 columns"><p>Leather: @Model.LeatherWearers</p></div>
                
                <div class="large-3 medium-3 columns"><p>Mail: @Model.MailWearers</p></div>
                
                <div class="large-3 medium-3 columns"><p>Plate: @Model.PlateWearers</p></div>
            </div>
        </div>
    </div>
</div>

<div class="panel">
    <div class="row">
        <h3>Tier Token Types</h3>
        <div class="large-12 medium-12 columns">
            <div class="large-12 medium-12 columns">
                <div class="large-4 medium-4 columns"><p>Vanquisher: @Model.VanquisherToken</p></div>

                <div class="large-4 medium-4 columns"><p>Conqueror: @Model.ConquerorToken</p></div>

                <div class="large-4 medium-4 columns"><p>Protector: @Model.ProtectorToken</p></div>
            </div>
        </div>
    </div>
</div>